<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.weather.featuretesting.presentation.custom_view.feature.view_finder_view.ViewFinderView
        android:id="@+id/viewFinderView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background_secondary"
        app:frameAspectRatioHeight="2"
        app:frameAspectRatioWidth="3"
        app:frameColor="@color/purple_200"
        app:frameCornersRadius="15dp"
        app:frameCornersSize="40dp"
        app:frameSize="0.85"
        app:frameThickness="2dp"
        app:indentLinesFrame="5dp"
        app:maskColor="@color/background_five" />

</androidx.constraintlayout.widget.ConstraintLayout>